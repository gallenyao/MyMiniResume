<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="litianly.myMiniResume">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="litianly.myMiniResume.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="litianly.myMiniResume.EducationEditActivity"/>
        <activity android:name="litianly.myMiniResume.ExperienceEditActivity"/>
        <activity android:name="litianly.myMiniResume.ProjectEditActivity"/>
        <activity android:name="litianly.myMiniResume.BasicInfoEditActivity"/>

    </application>

</manifest>